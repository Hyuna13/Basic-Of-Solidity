pragma solidity >= 0.7.0 < 0.9.0;

contract lec6{
    /*
    storage: 변수,함수들이 영속적으로 저장, 가스비용비쌈
    memory: 함수의 파라미터,리턴값,레퍼런스 타입저장, 영속적이지않고 함수내에서만 유효 storage보다 가스비용이 쌈
    Colldata: external function의 파라미터에서 사용
    stack: EVM에서 stack data를 관리할때 쓰는 영역인데 1024Mb 제한적
    */
    
    //function -string
    function get_string(string memory _str) public pure returns(string memory){
        return _str;
    }

    function get_uint(uint256 _ui) public pure returns(uint256){
        return _ui;
    }
}
